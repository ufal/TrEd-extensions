<?xml version="1.0" encoding="utf-8"?>
<pml_schema version="1.1" xmlns="http://ufal.mff.cuni.cz/pdt/pml/schema/">
    <revision>0.1.0</revision>
    <description>PADT d-level</description>

    <root name="DeepLevels" type="Trees.type"/>

    <type name="Trees.type">
        <structure>
            <member name="meta">
                <structure>
                    <member name="revision">
                        <cdata format="any"/>
                    </member>
                    <member name="date">
                        <cdata format="any"/>
                    </member>
                </structure>
            </member>
            <member name="data" required="1">
                <sequence role="#TREES">
                    <element name="Tree" type="Tree.type"/>
                </sequence>
            </member>
        </structure>
    </type>

    <type name="Tree.type">
        <structure role="#NODE">
            <member name="id" as_attribute="1" required="1" role="#ID"><cdata format="ID"/></member>
            <member name="ord" required="1" role="#ORDER"><cdata format="nonNegativeInteger"/></member>
            <member name="func" required="1"><constant>SENT</constant></member>
            <member name="comment" type="string.type"/>
            <member name="m" type="m.type"/>
            <member name="s" type="s.type"/>
            <member name="hide" type="string.type" role="#HIDE"/>
            <member name="with" role="#CHILDNODES">
                <sequence>
                    <element name="Node" type="Node.type"/>
                </sequence>
            </member>
        </structure>
    </type>

    <type name="Node.type">
        <structure role="#NODE">
            <member name="id" as_attribute="1" required="1" role="#ID"><cdata format="ID"/></member>
            <member name="ord" required="1" role="#ORDER"><cdata format="nonNegativeInteger"/></member>
            <member name="func" type="func.type" required="1"/>
            <member name="comment" type="string.type"/>
            <member name="context" type="context.type"/>
            <member name="parallel" type="parallel.type"/>
            <member name="paren" type="paren.type"/>
            <member name="arabfa" type="string.type"/>
            <member name="coref" type="string.type"/>
            <member name="clause" type="clause.type"/>
            <member name="m" type="m.type"/>
            <member name="s" type="s.type"/>
            <member name="hide" type="string.type" role="#HIDE"/>
            <member name="with" role="#CHILDNODES">
                <sequence>
                    <element name="Node" type="Node.type"/>
                </sequence>
            </member>
        </structure>
    </type>

